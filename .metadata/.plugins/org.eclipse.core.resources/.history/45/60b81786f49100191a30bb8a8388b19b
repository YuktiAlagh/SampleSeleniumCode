	public static void main(String args[])
	{
		fibonacciDisplay();
	}
	public static void fibonacciDisplay()
	{
		int i,n;
		for (i=0; i<=7;i++)
		{
			n=i-1;			
		}
		System.out.println(n+" ");	
		
	}
}
