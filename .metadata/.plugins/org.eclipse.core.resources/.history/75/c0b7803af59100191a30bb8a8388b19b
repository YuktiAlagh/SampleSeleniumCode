	public static void fibonacciDisplay()
	{
		int a[]=null,i,n=0;
		for (i=0;i<=7;i++)
		{
			n=i-1;
			a[i]=a[i]+a[i+1];
			//System.out.println(n+" ");
		}
			
		
	}
